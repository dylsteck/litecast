{
  "expo": {
    "name": "Litecast",
    "slug": "litecast",
    "version": "0.0.17",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.dylsteck.litecast"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "server",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      [
        "expo-router",
        {
          "origin": "https://litecast.xyz"
        }
      ],
      "expo-font",
      "expo-web-browser"
    ],
    "newArchEnabled": true,
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": "https://litecast.xyz"
      },
      "eas": {
        "projectId": "781a9530-5012-4c22-9f60-16f69a490c31"
      }
    },
    "owner": "dylsteck"
  }
}
